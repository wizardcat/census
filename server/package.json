{
  "name": "server",
  "version": "0.0.5",
  "main": "index.js",
  "description": "It's a server for the Census app",
  "author": "Roman Havrylko",
  "license": "MIT",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "build": "npx tsc",
    "type-check": "tsc --noEmit",
    "dev": "ts-node-dev --transpile-only --no-notify --exit-child src/index.ts",
    "generate": "ts-node --transpile-only src/schema.ts",
    "prisma": "npx prisma generate"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "axios": "^1.2.1",
    "axios-retry": "^3.3.1",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "iconv-lite": "^0.6.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.3",
    "concurrently": "^7.2.2",
    "nodemon": "^2.0.19",
    "prisma": "^4.0.0",
    "ts-node": "^10.8.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
